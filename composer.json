{
  "name": "gwsn/symfony-helpers",
  "description": "Simple helpers for Symfony to standardize the Api Responses and input validators",
  "license": "MIT",
  "homepage": "https://github.com/gwsn/symfony-helpers",
  "type": "symfony-bundle",
  "keywords": [
    "symfony",
    "validators",
    "Api Responses",
    "gwsn"
  ],
  "authors": [
    {
      "name": "Global Web Systems B.V.",
      "homepage": "https://www.globalwebsystems.nl"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "symfony/config": "^3.4||^4.0||^5.0||^6.0",
    "symfony/console": "^3.4||^4.0||^5.0||^6.0",
    "symfony/dependency-injection": "^3.4||^4.0||^5.0||^6.0",
    "symfony/event-dispatcher": "^3.4||^4.0||^5.0||^6.0",
    "symfony/http-kernel": "^3.4||^4.0||^5.0||^6.0",
    "symfony/security-core": "^3.4||^4.0||^5.0||^6.0",
    "symfony/http-foundation": "^3.4||^4.0||^5.0||^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "GWSN\\Helpers\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UnitTest\\GWSN\\Helpers\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "Composer\\Config::disableProcessTimeout",
      "vendor/bin/phpunit"
    ],
    "unit-test": "@test"
  },
  "config": {
    "sort-packages": true
  }
}
